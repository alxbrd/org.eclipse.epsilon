@model
operation loadHUTNModel() {
  loadHutn('TreeHUTN', '
@Spec {
	Metamodel {
		nsUri: "Tree"
	}
}
Model {
  Tree "t1" {
    label : "t1"
  }
  Tree "t2" {
    label : "t2"
    parent : Tree "t1"
  }
}
');
}

$with Sequence {"A", "TreeHUTN"}
operation hutnShouldBeEqualToItself() {
  assertEqualModels("A", "A");
}

$with Sequence {"A", "TreeHUTN", "B", "TreeEMF"}
$with Sequence {"A", "TreeEMF", "B", "TreeHUTN"}
@test
operation rawAndHUTNModelsShouldBeEqual() {
  assertEqualModels("A", "B");
}
