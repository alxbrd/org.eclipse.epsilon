pre {
	var unmatched = new Merged!t_unmatched;
	prepost.put("pre", "pre");
}

post {
	prepost.put("post", "post");
}

rule TreeWithTree
	merge l : Left!t_tree
	with r : Right!t_tree 
	into t : Merged!t_tree {
	
	if (l.parent.isUndefined()) {
		Merged.root = t;
		t.appendChild(unmatched);
	}
	else {
		l.parentNode.equivalent().appendChild(t);
	}
	
	t.a_label = l.a_label;
}

rule Tree2Tree
	transform s : Source!t_tree
	to t : Merged!t_tree {
	
	t.a_label = s.a_label;
	unmatched.appendChild(t);
	
}